	<cfif Application.fn_IsUserAuthorized("updateSubscriptionProcess") and Application.fn_IsUserAuthorized("processSubscriber")> | // <a href="index.cfm?method=subscription.processSubscriber&subscriberID=#URL.subscriberID#" title="Process subscriber subscriptions NOW, including generating invoice and processing payment. Only click if you really really mean it!" class="SubNavLink<cfif Variables.doAction is "processSubscriber">On</cfif>">Process Subscriber Now</a> \\</cfif>
</cfif>
</div><br>

<cfif IsDefined("URL.subscriptionID") and Application.fn_IsIntegerPositive(URL.subscriptionID) and IsDefined("qry_selectSubscription")>
	<div class="SubNav">
	<span class="SubNavObject">Subscription:</span> 
	<span class="SubNavName">
		<cfif qry_selectSubscription.subscriptionOrder is not 0>###qry_selectSubscription.subscriptionOrder#. </cfif>
		<cfif qry_selectSubscription.subscriptionID_custom is not "">#qry_selectSubscription.subscriptionID_custom#. </cfif>#qry_selectSubscription.subscriptionName#
	</span><br>
	<cfif Application.fn_IsUserAuthorized("viewSubscription")><a href="index.cfm?method=subscription.viewSubscription&subscriberID=#URL.subscriberID#&subscriptionID=#URL.subscriptionID#" title="View summary information of this subscription" class="SubNavLink<cfif Variables.doAction is "viewSubscription">On</cfif>">Summary</a></cfif> 
	<cfif Application.fn_IsUserAuthorized("updateSubscription") and qry_selectSubscription.subscriptionStatus is 1> | <a href="index.cfm?method=subscription.updateSubscription&subscriberID=#URL.subscriberID#&subscriptionID=#URL.subscriptionID#" title="Update subscription (archives this subscription and creates new version)" class="SubNavLink<cfif Variables.doAction is "updateSubscription">On</cfif>">Update</a></cfif> 
	<cfif Application.fn_IsUserAuthorized("insertSubscription")> | <a href="index.cfm?method=subscription.insertSubscription&subscriberID=#URL.subscriberID#&subscriptionID=#URL.subscriptionID#" title="Create new subscription for this subscriber as a copy of this subscription" class="SubNavLink<cfif Variables.doAction is "insertSubscription">On</cfif>">Copy</a></cfif>
	<cfif Application.fn_IsUserAuthorized("viewSubscription") and (Application.fn_IsUserAuthorized("viewCustomFieldValuesAll") or Application.fn_IsUserAuthorized("listStatusHistory"))> | <a href="index.cfm?method=subscription.viewSubscription&subscriberID=#URL.subscriberID#&subscriptionID=#URL.subscriptionID#&viewFieldArchives=True" title="View previous values for this subscription" class="SubNavLink<cfif Variables.doAction is "viewSubscription" and IsDefined("URL.viewFieldArchives") and URL.viewFieldArchives is True>On</cfif>">Archived Values</a></cfif> 
	<cfif Application.fn_IsUserAuthorized("listSalesCommissions")> | <a href="index.cfm?method=subscription.listSalesCommissions&subscriberID=#URL.subscriberID#&subscriptionID=#URL.subscriptionID#" title="View calculated sales commissions associated with this subscription" class="SubNavLink<cfif FindNoCase("Commission", Variables.doAction)>On</cfif>">Sales Commissions</a></cfif>
	<cfif qry_selectSubscription.subscriptionStatus is 1 and Application.fn_IsUserAuthorized("insertContact")> | <a href="index.cfm?method=subscription.insertContact&subscriberID=#URL.subscriberID#&subscriptionID=#URL.subscriptionID#" title="Create/send new contact management message associated with this subscription" class="SubNavLink<cfif FindNoCase("Contact", Variables.doAction)>On</cfif>">Contact Mgmt.</a></cfif>
	<cfif qry_selectSubscription.subscriptionStatus is 1 and Application.fn_IsUserAuthorized("insertNote")> | <a href="index.cfm?method=subscription.insertNote&subscriberID=#URL.subscriberID#&subscriptionID=#URL.subscriptionID#" title="Create and view notes associated with this subscription" class="SubNavLink<cfif FindNoCase("Note", Variables.doAction)>On</cfif>">Notes</a></cfif>
	<cfif qry_selectSubscription.subscriptionStatus is 1 and Application.fn_IsUserAuthorized("insertTask")> | <a href="index.cfm?method=subscription.insertTask&subscriberID=#URL.subscriberID#&subscriptionID=#URL.subscriptionID#" title="Create and view tasks associated with this subscription" class="SubNavLink<cfif FindNoCase("Task", Variables.doAction)>On</cfif>">Tasks</a></cfif>
	</div><br>
</cfif>
<cfif Variables.isViewPermission is True></form></cfif>
</cfoutput>
