	  <cfelseif Application.fn_IsUserAuthorized("viewSubscriberNotify")><a href="index.cfm?method=subscription.viewSubscriberNotify&subscriberID=#URL.subscriberID#" title="List the users who are sent invoices and other notifications for this subscriber" class="SubNavLink<cfif Variables.doAction is "viewSubscriberNotify">On</cfif>">List Existing Notified Users</a> | </cfif>
	<cfif Application.fn_IsUserAuthorized("insertSubscriberNotify")><a href="index.cfm?method=subscription.insertSubscriberNotify&subscriberID=#URL.subscriberID#" title="Add new user who is sent invoices and other notifications for this subscriber" class="SubNavLink<cfif Variables.doAction is "insertSubscriberNotify">On</cfif>">Add New Notified User</a> | </cfif>
	<cfif Application.fn_IsUserAuthorized("listInvoices")><a href="index.cfm?method=subscription.listInvoices&subscriberID=#URL.subscriberID#" title="List invoices for this subscriber" class="SubNavLink<cfif Find("Invoice", Variables.doAction) and Variables.doAction is not "insertInvoice">On</cfif>">List Existing Invoices</a> | </cfif>
	<cfif Application.fn_IsUserAuthorized("insertInvoice")><a href="index.cfm?method=subscription.insertInvoice&subscriberID=#URL.subscriberID#" title="Create new invoices for this subscriber" class="SubNavLink<cfif Variables.doAction is "insertInvoice">On</cfif>">Create New Invoice</a> | </cfif>
