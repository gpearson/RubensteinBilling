	<cfif Application.fn_IsUserAuthorized("updateSubscriber")><a href="index.cfm?method=subscription.updateSubscriber&subscriberID=#URL.subscriberID#" title="Update subscriber information" class="SubNavLink<cfif Variables.doAction is "updateSubscriber">On</cfif>">Update Subscriber</a> | </cfif>
