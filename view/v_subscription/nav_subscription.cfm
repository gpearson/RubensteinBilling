	  <cfelseif Application.fn_IsUserAuthorized("listNotes")><a href="index.cfm?method=subscription.listNotes&subscriberID=#URL.subscriberID#" title="View notes associated with this subscriber" class="SubNavLink<cfif Find("Note", Variables.doAction)>On</cfif>">Notes</a> | </cfif>
	<cfif Application.fn_IsUserAuthorized("insertTask")><a href="index.cfm?method=subscription.insertTask&subscriberID=#URL.subscriberID#" title="Create and view tasks associated with this subscriber" class="SubNavLink<cfif Find("Task", Variables.doAction)>On</cfif>">Tasks</a>
